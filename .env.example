# Z-Image API Configuration
# Copy this file to .env and fill in your actual API credentials

# SiliconFlow API (Primary - Z-Image Model)
IMAGE_API_URL=https://api.siliconflow.cn/v1/images/generations
IMAGE_API_KEY=your_siliconflow_api_key_here

# Model Settings (for SiliconFlow)
DEFAULT_MODEL=Qwen/Qwen-Image-Edit-2509
DEFAULT_WIDTH=1024
DEFAULT_HEIGHT=1024
DEFAULT_STEPS=20
DEFAULT_GUIDANCE=7.5

# Alternative API Configurations
# OpenAI DALL-E API
DALLE_API_URL=https://api.openai.com/v1/images/generations
DALLE_API_KEY=your_openai_api_key_here

# Stability AI API
STABILITY_API_URL=https://api.stability.ai/v1/generation
STABILITY_API_KEY=your_stability_api_key_here

# Midjourney API (if available)
MIDJOURNEY_API_URL=https://api.midjourney.com/v2
MIDJOURNEY_API_KEY=your_midjourney_api_key_here

# Custom API Configurations
CUSTOM_API_URL=
CUSTOM_API_KEY=

# API Settings
API_TIMEOUT=60000
API_RETRIES=3
MAX_CONCURRENT_REQUESTS=5